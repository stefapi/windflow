{
  "name": "windflow-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "WindFlow - Modern Docker Deployment Platform Frontend",
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc && vite build",
    "build-prod": "vite build --dest=../app/dist && cp -pr dist ../app",
    "preview": "vite preview",
    "preview:local": "PORT=3000 serve dist",
    "preview:dist": "PORT=3000 serve -s ../app/dist",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint-fix": "eslint --fix --ext \".vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts\" --ignore-pattern .gitignore .",
    "css-check": "npx stylelint \"**/*.{css,scss,vue}\"",
    "css-fix": "npx stylelint \"**/*.{css,scss,vue}\" --fix",
    "format": "prettier --write src/",
    "format:api": "prettier --write src/api/",
    "up": "taze major -I",
    "unlighthouse": "npx unlighthouse --site https://localhost:3000",
    "sizecheck": "npx vite-bundle-visualizer",
    "test": "vitest",
    "test:ci": "vitest --watch=false",
    "test:e2e": "start-server-and-test preview http://localhost:3001 'npx playwright test'",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3001 'npx playwright test'",
    "test:e2eui:dev": "start-server-and-test dev http://localhost:3001 'npx playwright test --ui'",
    "test:record:dev": "start-server-and-test dev http://localhost:3001 'npx playwright codegen http://localhost:3001'"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@jsonforms/core": "^3.6.0",
    "@jsonforms/vue": "^3.6.0",
    "@jsonforms/vue-vanilla": "^3.6.0",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.47.0",
    "@vue-flow/minimap": "^1.5.4",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "element-plus": "^2.11.4",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "pnpm": "^10.18.1",
    "sass-embedded": "^1.93.2",
    "ufo": "^1.6.1",
    "unhead": "^2.0.18",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/ep": "^1.2.3",
    "@iconify-json/fa": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/twemoji": "^1.2.4",
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.56.0",
    "@types/node": "^24.7.0",
    "@types/nprogress": "^0.2.3",
    "@unocss/reset": "^66.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.16",
    "@vue/tsconfig": "^0.8.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "eslint-plugin-vue": "^10.5.0",
    "https-localhost": "^4.7.1",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "puppeteer": "^24.23.0",
    "rollup": "^4.52.4",
    "rollup-plugin-visualizer": "^6.0.4",
    "shiki": "^3.13.0",
    "stylelint": "^16.25.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.1",
    "taze": "^19.7.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "unocss": "^66.5.2",
    "unocss-preset-element-plus": "^0.2.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-element-plus": "^0.10.0",
    "unplugin-icons": "^22.4.2",
    "unplugin-vue-components": "^29.1.0",
    "unplugin-vue-inspector": "^2.3.2",
    "unplugin-vue-jsx":  "^0.8.1",
    "unplugin-vue-macros": "^2.14.5",
    "unplugin-vue-markdown": "^29.2.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vite-plugin-webfont-dl": "^3.11.1",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.1",
    "workbox-build": "^7.3.0"
  }
}
